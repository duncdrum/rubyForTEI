      </rule>
   </pattern>
   <pattern id="schematron-constraint-ruby-lg-abstractModel-structure-l-19">
      <rule context="tei:lg">
         <report test="ancestor::tei:l[not(.//tei:note//tei:lg[. = current()])]">
