    >> sch:pattern [
         id = "ruby-damageSpan-spanTo_fr-constraint-assert-23"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:damageSpan"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "@spanTo"
             "L'attribut spanTo est requis."
           ]
